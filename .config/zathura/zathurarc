set statusbar-h-padding 0
set statusbar-v-padding 0
set page-padding 1
map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map i recolor
map p print

set default-bg    "#2F383E"
set statusbar-bg  "#2F383E"
set statusbar-fg  "#D3C6AA"
